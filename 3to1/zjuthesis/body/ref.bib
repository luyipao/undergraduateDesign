@article{wang2015stability,
  title     = {Stability and error estimates of local discontinuous Galerkin methods with implicit-explicit time-marching for advection-diffusion problems},
  author    = {Wang, Haijin and Shu, Chi-Wang and Zhang, Qiang},
  journal   = {SIAM Journal on Numerical Analysis},
  volume    = {53},
  number    = {1},
  pages     = {206--227},
  year      = {2015},
  publisher = {SIAM},
  abstract  = {本文考虑了几种特定的隐式-显式龙格-库塔时间推进方法，结合LDG方案来求解具有周期边界条件的线性对流-扩散问题。在这些方法中，扩散项以隐式方式处理，对流项以显式方式处理。通过建立数值解与其梯度之间的重要关系，并借助能量技术，我们证明了相应的IMEX LDG方案在时间步长限制$\tau \leq \tau_0$下是稳定的，其中常数\tau_0$仅依赖于对流和扩散系数，与网格尺寸h无关。在相同的时间条件$\tau \leq \tau_0$下，我们还提供了空间和时间上的最优误差估计。稳定性分析和误差估计可以推广到具有非线性对流部分的对流-扩散问题，并且类似的稳定性分析和误差估计也可以用于多步IMEX LDG格式，这些都构成了我们目前的研究工作。未来，我们希望考虑用于非周期边界条件的IMEX LDG方案，我们预计可以获得类似的稳定性，但精确的数值边界条件需要进行一些工作。}
}
@article{wang2016stability,
  title     = {Stability analysis and error estimates of local discontinuous Galerkin methods with implicit--explicit time-marching for nonlinear convection--diffusion problems},
  author    = {Wang, Haijin and Shu, Chi-Wang and Zhang, Qiang},
  journal   = {Applied Mathematics and Computation},
  volume    = {272},
  pages     = {237--258},
  year      = {2016},
  publisher = {Elsevier},
  abstract  = {Wang和Shu分析了周期性边界非线性对流扩散问题的集中特定的IMEX RK时间离散LDG法的稳定性和误差估计。在相同的时间条件下，如果采用一般单调数值通量，针对阶IMEX RK格式和三阶多步IMEX格式耦合LDG空间离散，给出了空间和时间上的最优误差估计。这也是本文中采用的IMEX RK时间离散方法。其分析推广到多维非线性对流-扩散问题需要一些技术处理，以获得数值解及其梯度之间的关键关系。同时，不能直接将误差估计从一维推广到多维，特别是对于一般的三角形网格。未来，我们希望考虑用于求解具有非线性扩散的非线性对流-扩散问题的IMEX LDG方法。}
}

