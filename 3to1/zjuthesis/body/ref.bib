@article{shu1988efficient,
  title     = {Efficient implementation of essentially non-oscillatory shock-capturing schemes},
  author    = {Shu, Chi-Wang and Osher, Stanley},
  journal   = {Journal of computational physics},
  volume    = {77},
  number    = {2},
  pages     = {439--471},
  year      = {1988},
  publisher = {Elsevier},
  abstract  = {我们通过直接在通量上使用移动模板的思想来简化ENO格式（essentially non-oscillatory），而不是在单元平均值，并通过构建一类适用于其他线性方案（如TVD、TVB方案）的TVD高阶Runge-Kutta类型时间离散化方法。我们可以使用不同的一阶E通量作为构建模块。其中一些（如Lax-Friedrichs）简单而平滑，而其他一些（如各种迎风通量）则更为复杂，在音速点不太平滑，但通常在非音速点上的耗散性更小。我们给出了四个显式的TVD Runge-Kutta公式（二阶到五阶），并在数值实验中使用其中两个（三阶和四阶）。对于系统的推广已经简要讨论，并将在未来的工作中进一步探索。数值结果表明，在平滑区域中具有良好的冲击转换而没有任何明显的振荡，并且具有高精度。与冲击相比，接触间断面更加模糊，这将通过某种形式的人工压缩来解决。通过数值实验验证了对于凸和非凸通量的正确熵解的收敛性。}
}
